{
  "extends": "@eulerxyz/eslint-config/react",
  "ignorePatterns": [
    "*.js",
    ".husky/",
    ".storybook/",
    "node_modules/",
    "dist/",
    "test/"
  ],
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/*.spec.*", "**/*.test.*", "**/*.stories.*"]}]
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
